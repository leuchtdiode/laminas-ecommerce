		"beberlei/doctrineextensions": "^1.2",
		"ramsey/uuid-doctrine": "^1.5",
		"firebase/php-jwt": "^5.0",
		"moneyphp/money": "^3.2",
		"spipu/html2pdf": "^5.2"
	},
	"suggest": {
		"mpay24/mpay24-php": "Mandatory if using mpay24 method handler",
